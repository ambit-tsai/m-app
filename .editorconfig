# http://editorconfig.org
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
quote_type=single
curly_bracket_next_line = false
indent_brace_style = 1tbs
[*.md]
trim_trailing_whitespace = false

[Makefile]
indent_style = tab
